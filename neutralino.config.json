{
  "applicationId": "neutralinojs.react.ts.vite.template",
  "version": "1.0.4",
  "defaultMode": "window",
  "documentRoot": "/react-src/build/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "singlePageServe": true,
  "tokenSecurity": "one-time",
  "nativeAllowList": ["app.*", "computer.*", "window.*"],
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "modes": {
    "window": {
      "title": "Neutralinojs + React + TS + Vite template",
      "width": 800,
      "height": 600,
      "minWidth": 800,
      "minHeight": 600,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/react-src/public/assets/images/vite.png",
      "enableInspector": false,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": false,
      "exitProcessOnClose": false,
      "useSavedState": false,
      "center": true
    }
  },
  "cli": {
    "binaryName": "application",
    "resourcesPath": "/react-src/build/",
    "extensionsPath": "/extensions/",
    "binaryVersion": "6.1.0",
    "clientVersion": "6.1.0",
    "frontendLibrary": {
      "patchFile": "/react-src/index.html",
      "devUrl": "http://localhost:3000",
      "projectPath": "/react-src/",
      "initCommand": "npm install",
      "devCommand": "npm run dev",
      "buildCommand": "npm run build"
    }
  }
}
